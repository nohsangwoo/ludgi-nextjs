{
  "name": "nextjs-2025",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "npm run prepare:dev && npm run start:dev",
    "prepare:dev": "npm run postbuild",
    "start:dev": "cross-env NODE_NO_WARNINGS=1 env-cmd -f .env nodemon --config nodemon.json --exec \"node --loader @swc-node/register/esm --experimental-specifier-resolution=node -r tsconfig-paths/register server/index.ts\"",
    "preinstall": "npx prisma generate",
    "prebuild": "node scripts/modifyPackageJson.cjs remove && npx prisma generate",
    "postbuild": "node scripts/modifyPackageJson.cjs restore",
    "build:server": "rimraf dist && swc server -d dist/ --copy-files",
    "start:server": "node dist/server/index.js",
    "build": "npm run prisma:generate && next build && npm run build:server",
    "start": "cross-env NODE_ENV=production npm run start:server",
    "lint": "next lint",
    "generate": "graphql-codegen --config codegen.yml",
    "codegen": "apollo client:codegen src/__generated__ --target=typescript --outputFlat",
    "analyze": "cross-env ANALYZE=true next build",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:push": "npx prisma db push",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:reset": "npx prisma migrate reset",
    "add-shadcn": "npx --legacy-peer-deps shadcn@latest add",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cy:open": "cypress open",
    "dev:watchgraphql": "env-cmd -f .env nodemon --watch server --watch server/graphql --ext ts,js,json,graphql --exec \"node -r @swc-node/register -r tsconfig-paths/register server/index.ts\""
  },
  "dependencies": {
    "@apollo/client": "^3.12.4",
    "@apollo/server": "^4.11.3",
